<% @plan.each do |plan| %>
  <%= I18n.t("activerecord.attributes.plan.name") %>
  <%= link_to plan.name , plan_path(plan.id) %>
  <%= I18n.t("activerecord.attributes.plan.content") %>
  <%= plan.content %>
  <%= I18n.t("activerecord.attributes.plan.updated_at") %>
  <%= format_datetime(plan.updated_at) %>
<% end %>